/* Especial
----------------------------------------------------*/
#Especial {
	margin:7px 0 10px 333px;
	padding-top:1px;
	padding-bottom:5px;
	border-top:solid 1px;
	border-bottom:solid 10px #007839;
	min-height:77px;
}
#Especial h2 {
	display:block;
	border-top:solid 10px #007839;
	height:0;
	overflow:hidden;
	margin:0 0 3px 0;
}
#Especial h3 {
	float:right;
	display:block;
	width:205px;
	*width:202px;
	text-align:left;
}
#Especial h3 a {
	font-size:100%;
	color:#007839;
}
#Especial img {
	float:left;
	margin:3px 5px 0 0;
}

/* Enquete
----------------------------------------------------*/
#Enquete {
	margin:10px 0 10px 333px;
	_margin:0 0 10px 333px;
	clear:right;
	background:url(http://diariodonordeste.globo.com/img/nuc/menu_titulos.jpg) repeat-x top;
/*	border-bottom:solid 3px #57AA54;*/
}
#Enquete legend {
	display:block;
	height:20px;
	font-family:"Trebuchet MS", Verdana;
	font-weight:bold;
	color:#FFF;
	text-transform:uppercase;
	padding:5px;
	*padding-left:0;
}
#Enquete p.pergunta {
	font-weight:bold;
	display:block;
	width:50%;
	float:left;
	margin:5px 0;
	*margin:0;
}
#Enquete p.opcoes {
	display:block;
	width:45%;
	float:left;
	margin:5px 0 5px 6px;
	*margin:0 0 0 6px;
	padding-left:5px;
	border-left:dotted 2px #999;
}
#Enquete p label {
	font-weight:normal;
}
#Enquete p.botao {
	margin-top:5px;
	float:left;
	clear:left;
	background:#57AA54;
}
#Enquete button {
	display:block;
	width:131px;
	*width:132px;
	clear:left;
	margin-left:145px;
	*margin-left:141px;
	border-left:solid 2px #FFF;
	background:#00814D;
	font-family:"Trebuchet MS", Verdana;
	font-size:10px;
	color:#FFFFFF;
	text-transform:uppercase;
}

/* Arte e Cultura
----------------------------------------------------*/
#Online #ArteCultura {
	width:130px;
	float:right;
	margin:0;
}

/* Entretenimento
----------------------------------------------------*/
#Online #Entretenimento {
	width:130px;
	float:right;
	margin:0;
	margin:0 15px 0 0;
}
#Online #Entretenimento div {
	background:#FFF;
}
#Online #Entretenimento h4 {
	margin:0;
	padding:7px 0 0 0;
}
#Online #Entretenimento p {
	margin:0;
	padding:0;
}

/* Alo Redacao
----------------------------------------------------*/
#Online #Aloredacao {
	margin:7px 0 10px 333px;
	clear:right;
}
#Online #Aloredacao h2 {
	display:block;
	border-top:solid 1px #000;
	padding-top:1px;
	margin:0;
}
#Online #Aloredacao h2 a {
	display:block;
	padding:2px 10px;
	background:#666;
/*	background:#836855;*/
	font-size:50%;
	text-transform:uppercase;
	color:#FFF;
}
#Online #Aloredacao li {
	border-bottom:solid 1px #666;
/*	border-bottom:solid 1px #836855;*/
	padding:2px 0;
}
#Online #Aloredacao strong {
	letter-spacing:-1px;
	padding-right:5px;
	text-transform:uppercase;
	color:#666;
/*	color:#836855;*/
}

/* Colunas
----------------------------------------------------*/
#Online #Colunas {
	margin:7px 0 10px 333px;
	clear:right;
}
#Online #Colunas h2 {
	display:block;
	border-top:solid 1px #000;
	padding-top:1px;
	margin:0 0 5px 0;
}
#Online #Colunas h2 a {
	display:block;
	padding:2px 10px;
	border-bottom:solid 1px #FFF;
	background:#007839;
	font-size:50%;
	text-transform:uppercase;
	color:#FFF;
}
#Online #Colunas li {
	float:left;
	width:133px;
	margin:0 5px 5px 0;
}
#Online #Colunas li a {
	background:#61A541;
	color:#FFF;
	display:block;
	height:31px;
	overflow:hidden;
	padding:5px 5px 0 5px;
	margin-right:39px;
	line-height:12px;
}

/* Adsense
----------------------------------------------------*/
#Online #Adsense {
	display:none;
}

/* Publicidade
----------------------------------------------------*/
#Online #Publicidade #Top {
	margin-left:0px;
	top:148px;
}
/* Menu
----------------------------------------------------*/
#Online #Menu {
	margin-top:75px;
}

/* ================================================= */
/*  CSS Blogs                                        */
/* ================================================= */
#Online #BlogCapa {
	padding-left:335px;
	min-height:160px;
	*min-height:auto;
}
#BlogCapa {
	font-family:"Trebuchet MS", Arial, Helvetica, sans-serif;
}
#BlogCapa h3 {
	background:url(http://diariodonordeste.globo.com/img/nuc/titulos_padrao.jpg) repeat-x top;
	padding:2px 3px;
	margin-bottom:5px;
	color:#FFF;
}
#BlogCapa h4 {
	padding:7px;
	min-height:30px;
	_height:30px;
	color:#FFF;
	font-size:90%;
}
#BlogCapa p {
	font-weight:bold;
	display:block;
	text-align:right;
	padding:3px 35px 3px 8px;
	_width:92px;
	_float:left;
	zoom:1;
}
#BlogChamada {
	width:100%;
	padding-bottom:5px;
	width:135px;
	float:left;
	margin-right:2px;
}
#BlogChamada a {
	color:#FFF;
	text-decoration:none;
	background:none;
}
#BlogChamada a:hover {
	text-decoration:underline;
}
